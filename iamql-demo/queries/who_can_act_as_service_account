whoCanActAsServiceAccount:
{
 p permission(name=="iam.serviceAccounts.actAs");
 r role;
 serviceaccount resource(type=="serviceaccount");
 ancestor resource;
 policy binding;
 m member;
}
{
 r.has(p);
 policy.grants(ancestor, r, m);
 ancestor.ancestorWithSelf(serviceaccount);
}
{
 m, serviceaccount
}
